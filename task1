def max_digits_not_dividable_by_3(n):
    max_digit = 0
    while n > 0:
        current = n % 10
        if current > max_digit and current % 3 != 0:
            max_digit = current
        n //= 10
    return max_digit

def mutual_simple(n):
    count = 0
    for i in range(2, n):
        if n % 2 == 0 and n % i == 0:
            count += 1
    return count


